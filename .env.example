E2E_KAM_USERNAME=kam@test.com
E2E_KAM_PASSWORD=Test@123456
E2E_CREDIT_USERNAME=credit@test.com
E2E_CREDIT_PASSWORD=Test@123456
E2E_NBFC_USERNAME=nbfc@test.com
E2E_NBFC_PASSWORD=Test@123456

# Backend Configuration
NODE_ENV=development
CORS_ORIGIN=http://localhost:8000

